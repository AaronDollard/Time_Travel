<Window x:Class="Time_Travel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Time_Travel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <ListBox x:Name="list_bx_world_countries" HorizontalAlignment="Left" Height="160" Margin="80,60,0,0" VerticalAlignment="Top" Width="240" SelectionChanged="world_countries_SelectionChanged"/>
        <ListBox x:Name="list_bx_world_bucket" HorizontalAlignment="Left" Height="160" Margin="480,60,0,0" VerticalAlignment="Top" Width="240"/>
        <ListBox x:Name="list_bx_world_visited" HorizontalAlignment="Left" Height="160" Margin="80,240,0,0" VerticalAlignment="Top" Width="240"/>
        <Button x:Name="bucket_add" Content="Add &gt;&gt;" HorizontalAlignment="Left" Height="20" Margin="360,100,0,0" VerticalAlignment="Top" Width="80"/>
        <Button x:Name="bucket_remove" Content="&lt;&lt; Remove" HorizontalAlignment="Left" Height="20" Margin="360,140,0,0" VerticalAlignment="Top" Width="80"/>
        <ComboBox x:Name="world_countries_filter" HorizontalAlignment="Left" Height="20" Margin="220,20,0,0" VerticalAlignment="Top" Width="100"/>

    </Grid>
</Window>
